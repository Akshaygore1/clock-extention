{
  "manifest_version": 3,
  "name": "Custom Startup Page",
  "version": "1.0",
  "description": "A custom startup page for Chrome.",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
